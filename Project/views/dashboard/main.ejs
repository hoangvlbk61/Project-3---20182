                    <%
                    var canduoi;
                    var cantren=data.entry_score;
                    if(cantren%50===0){
                        canduoi=cantren-50;
                    }else{
                        canduoi=Math.floor(cantren/50)*50;
                    }
                    var distance=Math.floor((data.target_score-data.entry_score)/50);
                    var start=canduoi+100;

                    %>
                    <div class="milestone-process hidden-lg hidden-md">
                        <ul>
                            <li><span> <%=canduoi %> - <%=cantren %></span></li>
                            <li>
                                <img src="/static/LearningPlan/images/new/icon_lo_trinh_hoc.png" alt="">
